<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
    android:background="@color/white">
    <RelativeLayout
        android:id="@+id/logistics_name_rl"
        style="@style/mine_item_style02"
        android:paddingLeft="5dp"
        android:paddingRight="10dp"
        >
        <TextView
            android:id="@+id/consignee_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/consignee"
            android:textSize="25dp"
            android:textColor="@color/fontColor2"
            android:layout_toRightOf="@id/coordinate_iv"
            android:layout_marginLeft="10dp"

            android:layout_marginTop="10dp"/>
        <TextView
            android:id="@+id/consignee_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="小五"
            android:textSize="25dp"
            android:textColor="@color/fontColor2"
            android:layout_toRightOf="@id/consignee_title_tv"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"/>
        <TextView
            android:id="@+id/phone_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="131358745454"
            android:textSize="25dp"
            android:textColor="@color/fontColor2"
            android:layout_marginLeft="10dp"
            android:layout_alignParentRight="true"
            android:layout_marginTop="10dp"/>
        <TextView
            android:id="@+id/address_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_address02"
            android:textSize="25dp"
            android:textColor="@color/fontColor6"
            android:layout_toRightOf="@id/coordinate_iv"
            android:layout_below="@id/consignee_title_tv"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"/>
        <TextView
            android:id="@+id/address_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="广东省佛山市禅城区"
            android:textSize="25dp"
            android:textColor="@color/fontColor6"
            android:layout_toRightOf="@id/address_title_tv"
            android:layout_below="@id/consignee_title_tv"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"/>
    </RelativeLayout>
    <View
        android:layout_marginTop="3dp"
        style="@style/view_style"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingLeft="5dp"
        android:paddingRight="10dp"
        android:gravity="center_vertical">
        <TextView
            android:id="@+id/default_addr_tv"
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/defaults_address"
            android:textSize="26dp"
            android:textColor="@color/colorPrimaryRed"
            />
        <TextView
            android:id="@+id/edit_tv"
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="编辑"
            android:textSize="25dp"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/background_color"/>
</LinearLayout>